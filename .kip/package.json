{
  "name": "@kk0x03/kip",
  "version": "2.0.1",
  "description": "A simple, minimal blog framework for showcasing your projects",
  "type": "module",
  "bin": {
    "kip": "./bin/kip.js"
  },
  "files": [
    "bin/",
    "scripts/",
    "src/",
    "public/",
    "blog/",
    ".github/",
    "_config.yml",
    "index.html",
    "vite.config.ts",
    "tsconfig.json",
    "tsconfig.node.json",
    "eslint.config.js",
    "template/"
  ],
  "scripts": {
    "dev": "tsx scripts/build-content.ts && vite",
    "build": "tsx scripts/build-content.ts && vite build",
    "preview": "vite preview --base=/blog/",
    "prepublishOnly": "npm run build",
    "test:init": "rm -rf dev/* && KIP_CWD=$(pwd)/dev npx tsx bin/kip.ts init",
    "test:dev": "cd dev && npm install && npm run dev"
  },
  "keywords": [
    "blog",
    "portfolio",
    "react",
    "vite",
    "typescript",
    "markdown",
    "github-pages",
    "cli"
  ],
  "author": "kk0x03",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kk0x03/kip.git"
  },
  "bugs": {
    "url": "https://github.com/kk0x03/kip/issues"
  },
  "homepage": "https://github.com/kk0x03/kip#readme",
  "dependencies": {
    "@notionhq/client": "^5.4.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "marked": "^17.0.1",
    "notion-to-md": "^3.1.9",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.15.29",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^7.2.4"
  }
}
